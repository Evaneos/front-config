{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": true,
    "module": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "inlineSources": true,
    "inlineSourceMap": false
  }
}
